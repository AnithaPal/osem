.container
- if @conference.present?
  .row
    .col-md-12
    - if @program.present?
      %section#program#current-events
        = render 'current_events'
  .row
    .col-md-12.text-center
      %h2 Tweets About Conference
  .row
    .col-md-12
    - if @sponsors.any?
      %section#sponsors
        = render 'conference/sponsors'
